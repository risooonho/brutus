<div id="searches" class="setHeight"	 onmouseover="$('searches').toggleClassName('setHeight');"	 onmouseout="$('searches').toggleClassName('setHeight');">	{foreach from=$stored_searches item="search" key="label"}		{if $search|@is_array}			<a href="javascript:$('q').value='{$search.term}';$('type').value='{$search.type}';submitAjaxSearchFormExtended($('searchform'));void(0);"			   class="searchterm channel" title="{$search.alert}">{$label}</a>		{elseif $search != ""}			<a href="javascript:$('q').value='{$search}';submitAjaxSearchForm($('searchform'));void(0);"			   class="searchterm">{$search|ucfirst}</a>		{/if}	{foreachelse}			{/foreach}</div>