<div class="termlist" id="tl{$results|@serialize|md5}">	{if $fieldA != ""}		<table>				<tr>					<th>{$fieldA}</th>					<th>{$fieldB}</th>				</tr>	{else}		<ul>	{/if}	{foreach from=$results item="result" key="label" name="termlist"}		{cycle values="odd,even" name="pete" assign="bg"}		{if $fieldA != ""}			<tr>				<td><a href="/{$basedir}index.php?q={$result.$fieldA}">{$result.$fieldA|ucfirst}</a></td>				<td>				{if $fieldB == "Date"}					{$result.$fieldB|date_format}				{else}					{$result.$fieldB}				{/if}				</td>			</tr>		{else}			{if $result|@is_array}				<li><a href="/{$basedir}index.php?verb=SearchResults&q={$result.term}&type={$result.type}"				   class="searchterm channel" title="{$result.alert}">{$label}</a></li>			{else}				<li><a href="/{$basedir}index.php?q={$result}">{$result|ucfirst}</a></li>			{/if}		{/if}	{/foreach}	{if $fieldA != ""}		</table>	{else}		</ul>	{/if}</div>